{
  "name": "KateBookmarkPlugin",
  "files": [
    {
      "git": 1
    }
  ],
  "exclude_patterns": [ "^po" ],
  "build": {
    "directory": "build",
    "default_target": "cmake-build",
    "targets": [
      {
        "name": "cmake-init",
        "build_cmd": "cmake .."
      },
      {
        "name": "cmake-build",
        "build_cmd": "cmake --build ."
      },
      {
        "name": "cmake-install",
        "build_cmd": "sudo cmake --install ."
      }
    ]
  }
}